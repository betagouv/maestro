{
  "name": "PSPC",
  "version": "0.0.1",
  "main": "server/index.ts",
  "scripts": {
    "frontend:build": "npm --prefix frontend install --f && npm run --prefix frontend build",
    "frontend:start": "npm --prefix frontend start",
    "frontend:lint": "npm --prefix frontend run lint",
    "frontend:test": "npm --prefix frontend test",
    "server-local": "npm run migrate-latest && ts-node-dev --respawn --transpile-only -r dotenv/config server/index.ts",
    "start-local": "run-p frontend:start server-local",
    "clean": "rimraf dist",
    "copy": "copyfiles -V 'server/**/*.css' 'server/**/*.hbs' dist",
    "prebuild": "run-s clean copy",
    "build": "npm run migrate-latest && tsc && npm run copy && npm run frontend:build",
    "start": "node ./dist/server/index.js",
    "test": "jest",
    "lint": "eslint . --ext .js,.ts",
    "migrate-list": "knex migrate:list --knexfile=./server/knex.ts",
    "migrate-latest": "knex migrate:latest --knexfile=./server/knex.ts",
    "migrate-rollback-one": "knex migrate:down --knexfile=./server/knex.ts",
    "migrate-rollback-all": "knex migrate:rollback --all --knexfile=./server/knex.ts",
    "seed": "knex seed:run --knexfile=./server/knex.ts"
  },
  "license": "AGPL-3.0",
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.624.0",
    "@aws-sdk/s3-request-presigner": "^3.624.0",
    "@faker-js/faker": "^8.2.0",
    "@getbrevo/brevo": "^2.1.1",
    "@jest-mock/express": "^1.4.5",
    "@sentry/node": "^7.118.0",
    "bcryptjs": "^2.4.3",
    "compose-middleware": "^5.0.1",
    "convict": "^6.2.4",
    "convict-format-with-validator": "^6.2.0",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^10.0.0",
    "exceljs": "^4.4.0",
    "express": "^4.21.0",
    "express-async-errors": "^3.1.1",
    "express-fileupload": "^1.4.0",
    "express-jwt": "^8.4.1",
    "express-rate-limit": "^6.4.0",
    "form-data": "^4.0.0",
    "handlebars": "^4.7.8",
    "helmet": "^5.0.2",
    "highland": "^2.13.5",
    "http-proxy-middleware": "^3.0.3",
    "jest": "^29.3.1",
    "jest-fetch-mock": "^3.0.3",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "knex-stringcase": "^1.4.6",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.7",
    "nodemailer": "^6.9.13",
    "pg": "^8.7.3",
    "puppeteer": "^22.11.2",
    "randomstring": "^1.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "uuid": "^8.3.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/convict": "^6.1.1",
    "@types/convict-format-with-validator": "^6.0.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.4.1",
    "@types/highland": "^2.13.0",
    "@types/jest": "^29.2.4",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.30",
    "@types/node-fetch": "^2.5.12",
    "@types/nodemailer": "^6.4.15",
    "@types/randomstring": "^1.1.8",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.7",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "lint-staged": "^13.1.0",
    "nodemailer-mock": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "rimraf": "^5.0.7",
    "supertest": "^6.2.4",
    "ts-essentials": "^9.3.0",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^1.1.8",
    "type-fest": "^3.5.0"
  }
}
