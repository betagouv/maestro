{
  "name": "Maestro",
  "version": "0.0.1",
  "main": "./server/index.js",
  "type": "module",
  "scripts": {
    "frontend:build": "npm --prefix frontend ci --include=dev && npm run --prefix frontend build",
    "frontend:start": "npm --prefix frontend start",
    "frontend:lint": "npm --prefix frontend run lint",
    "frontend:test": "npm --prefix frontend test",
    "server-local": "npm run dev",
    "dev": "npm run migrate-latest && node --import @swc-node/register/esm-register --watch --inspect ./server/index.ts",
    "start-local": "run-p frontend:start server-local",
    "build": "tsc  && npm run frontend:build",
    "start": "npm run migrate-latest && node --import @swc-node/register/esm-register ./server/index.ts",
    "test": "vitest",
    "test:integration": "vitest --project integration",
    "test:unit": "vitest --project unit",
    "lint": "eslint . --ext .js,.ts",
    "migrate-list": "NODE_OPTIONS='--import @swc-node/register/esm-register' knex migrate:list --knexfile=./server/knex.ts",
    "migrate-latest": "NODE_OPTIONS='--import @swc-node/register/esm-register' knex migrate:latest --knexfile=./server/knex.ts",
    "migrate-rollback-one": "NODE_OPTIONS='--import @swc-node/register/esm-register' knex migrate:down --knexfile=./server/knex.ts",
    "migrate-rollback-all": "NODE_OPTIONS='--import @swc-node/register/esm-register' knex migrate:rollback --all --knexfile=./server/knex.ts",
    "seed": "node --import @swc-node/register/esm-register ./database/seeds/dummy/index.ts",
    "kysely-generate": "kysely-codegen --camel-case --out-file server/repositories/kysely.type.ts"
  },
  "license": "AGPL-3.0",
  "engines": {
    "node": "22",
    "npm": "10"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.0",
    "@aws-sdk/client-s3": "^3.624.0",
    "@aws-sdk/s3-request-presigner": "^3.624.0",
    "@faker-js/faker": "^8.2.0",
    "@getbrevo/brevo": "^2.1.1",
    "@sentry/node": "^7.118.0",
    "@swc-node/register": "^1.10.9",
    "@swc/core": "^1.10.1",
    "compose-middleware": "^5.0.1",
    "convict": "^6.2.4",
    "convict-format-with-validator": "^6.2.0",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^10.0.0",
    "exceljs": "^4.4.0",
    "express": "^4.21.0",
    "express-async-errors": "^3.1.1",
    "express-fileupload": "^1.4.0",
    "express-jwt": "^8.4.1",
    "express-rate-limit": "^6.4.0",
    "form-data": "^4.0.0",
    "handlebars": "^4.7.8",
    "helmet": "^5.0.2",
    "highland": "^2.13.5",
    "http-proxy-middleware": "^3.0.3",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "knex-stringcase": "^1.5.5",
    "kysely": "^0.27.4",
    "lodash-es": "^4.17.21",
    "node-fetch": "^2.6.7",
    "nodemailer": "^6.9.13",
    "openid-client": "^5.7.1",
    "pg": "^8.13.1",
    "puppeteer": "^22.11.2",
    "randomstring": "^1.2.1",
    "typescript": "^5.7.2",
    "uuid": "^8.3.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/convict": "^6.1.1",
    "@types/convict-format-with-validator": "^6.0.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.4.1",
    "@types/highland": "^2.13.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.30",
    "@types/node-fetch": "^2.5.12",
    "@types/nodemailer": "^6.4.17",
    "@types/openid-client": "^3.1.6",
    "@types/pg": "^8.11.10",
    "@types/randomstring": "^1.1.8",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.7",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "kysely-codegen": "^0.17.0",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "3.4.0",
    "prettier-plugin-organize-imports": "^4.1.0",
    "supertest": "^6.2.4",
    "ts-essentials": "^9.3.0",
    "type-fest": "^3.5.0",
    "vitest": "^2.1.8"
  }
}
