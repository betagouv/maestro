{
  "name": "maestro-server",
  "version": "0.0.1",
  "main": "./index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "npm run migrate-latest && node --import @swc-node/register/esm-register --watch --inspect ./index.ts",
    "build": "tsc",
    "start": "npm run migrate-latest && node --import @swc-node/register/esm-register ./index.ts",
    "test": "vitest",
    "test:integration": "vitest --project integration",
    "test:unit": "vitest --project unit",
    "lint": "eslint . --ext .js,.ts",
    "migrate-list": "NODE_OPTIONS='--import @swc-node/register/esm-register' knex migrate:list",
    "migrate-latest": "NODE_OPTIONS='--import @swc-node/register/esm-register' knex migrate:latest",
    "migrate-rollback-one": "NODE_OPTIONS='--import @swc-node/register/esm-register' knex migrate:down",
    "migrate-rollback-all": "NODE_OPTIONS='--import @swc-node/register/esm-register' knex migrate:rollback --all",
    "seed": "node --import @swc-node/register/esm-register database/seeds/dummy/index.ts",
    "kysely-generate": "kysely-codegen --camel-case --out-file repositories/kysely.type.ts",
    "ssd2-generate": "node --import @swc-node/register/esm-register ./scripts/updateSSD2Referential.ts",
    "check:emails": "node --import @swc-node/register/esm-register ./services/imapService/script.ts"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.0",
    "@aws-sdk/client-s3": "^3.624.0",
    "@aws-sdk/s3-request-presigner": "^3.624.0",
    "@faker-js/faker": "^8.2.0",
    "@getbrevo/brevo": "^2.1.1",
    "@gouvfr/dsfr": "^1.13.0",
    "@sentry/node": "^7.118.0",
    "@swc-node/register": "^1.10.9",
    "@swc/core": "^1.10.1",
    "carbone": "^3.5.6",
    "compose-middleware": "^5.0.1",
    "convict": "^6.2.4",
    "convict-format-with-validator": "^6.2.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "exceljs": "^4.4.0",
    "express": "^4.21.0",
    "express-async-errors": "^3.1.1",
    "express-fileupload": "^1.4.0",
    "express-jwt": "^8.4.1",
    "express-rate-limit": "^6.4.0",
    "fast-xml-parser": "^4.5.1",
    "form-data": "^4.0.0",
    "handlebars": "^4.7.8",
    "helmet": "^5.0.2",
    "highland": "^2.13.5",
    "http-proxy-middleware": "^3.0.3",
    "imapflow": "^1.0.179",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "knex-stringcase": "^1.5.5",
    "kysely": "^0.27.4",
    "maestro-shared": "^0.0.1",
    "mailparser": "^3.7.2",
    "node-fetch": "^2.6.7",
    "nodemailer": "^6.9.13",
    "openid-client": "^5.7.1",
    "pg": "^8.13.1",
    "puppeteer": "^22.11.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/carbone": "^3.2.5",
    "@types/convict": "^6.1.1",
    "@types/convict-format-with-validator": "^6.0.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.4.1",
    "@types/highland": "^2.13.0",
    "@types/imapflow": "^1.0.19",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mailparser": "^3.4.5",
    "@types/node": "^20.11.30",
    "@types/node-fetch": "^2.5.12",
    "@types/nodemailer": "^6.4.17",
    "@types/openid-client": "^3.1.6",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.7",
    "kysely-codegen": "^0.17.0",
    "supertest": "^6.2.4",
    "ts-essentials": "^9.3.0",
    "type-fest": "^3.5.0"
  }
}
