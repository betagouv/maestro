{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Node",
    "composite": true,
    "incremental": true,
    "strict":true,

    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noEmit": true,
    "allowJs": true
  }
}