{
  "env": {
    "APPLICATION_HOST": {
      "generator": "url",
      "template": "%URL%"
    },
    "REACT_APP_PUBLIC_URL": {
      "generator": "url",
      "template": "%URL%"
    },
    "REACT_APP_API_URL": {
      "generator": "url",
      "template": "%URL%"
    }
  },
  "scripts": {
    "first-deploy": "npm ci --production=false && npm run migrate-latest && npm run seed"
  },
  "jobs": [{
    "command": "*/10 * * * * curl ${REACT_APP_API_URL}/checkLaboratoryEmails -H 'authorization: ${MAESTRO_BASIC_TOKEN}",
    "size": "M"
  }]
}
